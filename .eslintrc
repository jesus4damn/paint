{
    "parser": "babel-eslint",
    "env":{
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules":{
        "semi":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "no-trailing-spaces":"off",
        "eol-last":"off",
        "indent":"off",
        "object-curly-spacing":"off",
        "operator-linebreak":"off",
        "no-unused-vars":"off",
        "no-empty":"off"
    },
    "extends":["eslint:recommended", "google"]
}